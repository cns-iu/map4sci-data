\COPY (SELECT DISTINCT(medline_master.pmid) AS pmid, medline_master.article_title AS Title, medline_abstract_text.abstract_text AS Abstract, medline_master.pub_year AS Year, wos_cited.cite_count AS Citation_count FROM medline_master JOIN medline_abstract_text ON medline_master.pmid = medline_abstract_text.pmid JOIN wosid_to_pmid ON wosid_to_pmid.pmid = medline_master.pmid JOIN wos_cited ON wosid_to_pmid.wosid = wos_cited.id WHERE pub_year::int <= 2018 AND pub_year::int >= 2009) TO '/people/maahutch/pubmed_node_edge/node_2009_2018.csv' CSV HEADER; 
